/* @group Main */

.textformat * {
	margin: 0;
	padding: 0;
}

.textformat p {
	font: 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #ccc;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
}

.textformat li {
	font: 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #ccc;
	text-shadow: #000 0 0 0;
}

.textformat ul {
	margin-bottom: 18px;
	margin-left: 36px;
	list-style-type: disc;
	list-style-position: outside;
	margin-right: 18px;
}

.textformat ol {
	margin-bottom: 18px;
	margin-left: 36px;
	list-style-type: decimal;
	list-style-position: outside;
	margin-right: 18px;

}

.textformat ol ol, .textformat ul ul, .textformat ol ul, .textformat ul ol {
	margin-left: 18px;
	margin-bottom: 0px;
}

.textformat dl {
	margin-bottom: 18px;
	margin-left: 18px;
	margin-right: 18px;
	font: 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #ccc;
	text-shadow: #000 0 0 0;
}

.textformat dl dd {
	margin-left: 18px;
}

.textformat dl dt {
	font-weight: bold;
}

.textformat em{
	font-style: italic;
}

.textformat strong{
	font-weight: bold;
	background-color: #171717;
	padding-right: 2px;
	padding-left: 2px;
	color: #f8f8f8;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

.textformat code{
	font: 13px "Courier New", Courier, mono;
	color: #ccc;
	background-color: #171717;
	padding-right: 3px;
	padding-left: 3px;
	text-shadow: none;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

.textformat pre{
	font: 11px/18px "Courier New", Courier, mono;
	color: #ccc;
	background-color: #171717;
	text-shadow: none;
	padding: 7px 10px;
	margin-bottom: 18px;
	overflow: auto;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

.textformat pre code{
	font: 12px/18px "Courier New", Courier, mono;
}

.textformat a {
	font-weight: bold;
	color: #009d20;
	text-decoration: none;
}

.textformat a:visited {
	color: #006900;
}

.textformat a:hover {
	color: #fdff44;
}

.textformat blockquote {
	color: #707070;
	font-style: italic;
	padding-left: 6px;
	margin-left: 18px;
	border-left: 2px solid #666;
	margin-right: 18px;
}

.textformat blockquote p, .textformat blockquote li {
	color: #707070;
	font-style: italic;
}

.textformat del {
	color: #666;
	text-decoration: line-through;
}

.textformat dfn {
	font-weight: bold;
	font-style: italic;
}

.textformat abbr, .textformat acronym {
	border-bottom: 1px dotted #666;
}

.textformat address {
	font: italic 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #666;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
	margin-left: 18px;
}

.textformat hr {
	border-bottom: 1px solid #191919;
	margin-bottom: 18px;
	clear: both;
}

.textformat hr.small {
	width: 200px;
}

.textformat h1 {
	color: #fff;
	text-shadow: #000 0 0 0;
	font: 32px/36px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	margin-bottom: 18px;
}

.textformat h2 {
	color: #fff;
	text-shadow: #000 0 0 0;
	font: 27px/36px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	margin-bottom: 18px;
}

.textformat h3 {
	color: #fff;
	font: 22px/36px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
}

.textformat h4 {
	color: #fff;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
	font: bold 18px/36px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}

.textformat h5 {
	color: #fff;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
	font: bold 15px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}

.textformat h6 {
	color: #fff;
	margin-bottom: 18px;
	text-shadow: #000 0 0 0;
	font: bold 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}

.textformat sub, .textformat sup {
	font-size: 75%;
	line-height: 30%;
}

.textformat .box {
	background-color: #171717;
	margin-bottom: 18px;
	padding: 18px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

.textformat .box p {
	margin-bottom: 0;
}

.textformat .right {
	float: right;
	width: 200px;
	margin-left: 18px;
}

.textformat .left {
	float: left;
	width: 200px;
	margin-right: 18px;
}

.textformat img.left, .textformat img.right {
	margin-bottom: 18px;
}

.textformat ol.footnotes li, .textformat .footnote ol li, .textformat .footnote ol li p {
	font-size: 10px;
	line-height: 18px;
	color: #7e7e7e;
}

.textformat .footnote hr {
	width: 200px;
}



/* @end */

/* @group Typografi */


.caps {
	font-size: 90%;
}

.dquo {
	margin-left: -0.4em;
}

.quo {
	margin-left: -0.22em;
}

/* @end */

/* @group Table */

.textformat table {
	font: 13px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: #000 0 0 0;
	margin-bottom: 18px;
	margin-right: auto;
	margin-left: auto;
	border-top: 1px solid #333;
	border-left: 1px solid #333;
}

.textformat table.has_first_last {
	border-top: none;
	border-left: none;
	-moz-border-radius-topleft: 4px;
	-webkit-border-top-left-radius: 4px;
	-moz-border-radius-topright: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-bottom-left-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	-webkit-border-bottom-right-radius: 4px;
}

.textformat table tr.first td, .textformat table tr.first th {
	border-top: 1px solid #333;
}

.textformat table tr td.first, .textformat table tr th.first {
	border-left: 1px solid #333;
}

.textformat table tr.first td.last, .textformat table tr.first th.last {
	-moz-border-radius-topright: 4px;
	-webkit-border-top-right-radius: 4px;
}

.textformat table tr.first td.first, .textformat table tr.first th.first {
	-moz-border-radius-topleft: 4px;
	-webkit-border-top-left-radius: 4px;
}

.textformat table tr.last td.last, .textformat table tr.last th.last {
	-moz-border-radius-bottomright: 4px;
	-webkit-border-bottom-right-radius: 4px;
}

.textformat table tr.last td.first, .textformat table tr.last th.first {
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-bottom-left-radius: 4px;
}

.textformat table caption {
	font-style: italic;
	font-size: 13px;
	font-weight: bold;
}

.textformat table th {
	border-right: 1px solid #333;
	border-bottom: 1px solid #333;
	padding: 2px 8px;
	text-align: center;
	background-color: #333;
}

.textformat table td {
	border-right: 1px solid #333;
	border-bottom: 1px solid #333;
	padding: 2px 4px;
}

.textformat table tfoot {
	font-style: italic;
}



/* @end */

/* @group Code hilight */

table.codehilitetable {
	text-align: left;
	margin: 0 0 18px;
	padding: 0;
	width: 100%;
	height: auto;
	border-style: none;
	background-color: #171717;
	color: #ccc;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

table.codehilitetable td, table.codehilitetable tr {
	text-align: left;
	margin: 0;
	padding: 0;
	width: auto;
	height: auto;
	border-style: none;
	vertical-align: top;
}

table.codehilitetable .linenos {
	border-right: 1px solid #262626;
	width: 1%;
}

table.codehilitetable .code .codehilite {
	
}

table.codehilitetable pre {
	margin-bottom: 0;
	padding-bottom: 1;
}

codehilite  { background: #f8f8f8; }
.codehilite .c { color: #408080; font-style: italic } /* Comment */
.codehilite .err { border: 1px solid #FF0000 } /* Error */
.codehilite .k { color: #008000; font-weight: bold } /* Keyword */
.codehilite .o { color: #666666 } /* Operator */
.codehilite .cm { color: #408080; font-style: italic } /* Comment.Multiline */
.codehilite .cp { color: #BC7A00 } /* Comment.Preproc */
.codehilite .c1 { color: #408080; font-style: italic } /* Comment.Single */
.codehilite .cs { color: #408080; font-style: italic } /* Comment.Special */
.codehilite .gd { color: #A00000 } /* Generic.Deleted */
.codehilite .ge { font-style: italic } /* Generic.Emph */
.codehilite .gr { color: #FF0000 } /* Generic.Error */
.codehilite .gh { color: #000080; font-weight: bold } /* Generic.Heading */
.codehilite .gi { color: #00A000 } /* Generic.Inserted */
.codehilite .go { color: #808080 } /* Generic.Output */
.codehilite .gp { color: #000080; font-weight: bold } /* Generic.Prompt */
.codehilite .gs { font-weight: bold } /* Generic.Strong */
.codehilite .gu { color: #800080; font-weight: bold } /* Generic.Subheading */
.codehilite .gt { color: #0040D0 } /* Generic.Traceback */
.codehilite .kc { color: #008000; font-weight: bold } /* Keyword.Constant */
.codehilite .kd { color: #008000; font-weight: bold } /* Keyword.Declaration */
.codehilite .kp { color: #008000 } /* Keyword.Pseudo */
.codehilite .kr { color: #008000; font-weight: bold } /* Keyword.Reserved */
.codehilite .kt { color: #B00040 } /* Keyword.Type */
.codehilite .m { color: #666666 } /* Literal.Number */
.codehilite .s { color: #BA2121 } /* Literal.String */
.codehilite .na { color: #7D9029 } /* Name.Attribute */
.codehilite .nb { color: #008000 } /* Name.Builtin */
.codehilite .nc { color: #0000FF; font-weight: bold } /* Name.Class */
.codehilite .no { color: #880000 } /* Name.Constant */
.codehilite .nd { color: #AA22FF } /* Name.Decorator */
.codehilite .ni { color: #999999; font-weight: bold } /* Name.Entity */
.codehilite .ne { color: #D2413A; font-weight: bold } /* Name.Exception */
.codehilite .nf { color: #0000FF } /* Name.Function */
.codehilite .nl { color: #A0A000 } /* Name.Label */
.codehilite .nn { color: #0000FF; font-weight: bold } /* Name.Namespace */
.codehilite .nt { color: #008000; font-weight: bold } /* Name.Tag */
.codehilite .nv { color: #19177C } /* Name.Variable */
.codehilite .ow { color: #AA22FF; font-weight: bold } /* Operator.Word */
.codehilite .w { color: #bbbbbb } /* Text.Whitespace */
.codehilite .mf { color: #666666 } /* Literal.Number.Float */
.codehilite .mh { color: #666666 } /* Literal.Number.Hex */
.codehilite .mi { color: #666666 } /* Literal.Number.Integer */
.codehilite .mo { color: #666666 } /* Literal.Number.Oct */
.codehilite .sb { color: #BA2121 } /* Literal.String.Backtick */
.codehilite .sc { color: #BA2121 } /* Literal.String.Char */
.codehilite .sd { color: #BA2121; font-style: italic } /* Literal.String.Doc */
.codehilite .s2 { color: #BA2121 } /* Literal.String.Double */
.codehilite .se { color: #BB6622; font-weight: bold } /* Literal.String.Escape */
.codehilite .sh { color: #BA2121 } /* Literal.String.Heredoc */
.codehilite .si { color: #BB6688; font-weight: bold } /* Literal.String.Interpol */
.codehilite .sx { color: #008000 } /* Literal.String.Other */
.codehilite .sr { color: #BB6688 } /* Literal.String.Regex */
.codehilite .s1 { color: #BA2121 } /* Literal.String.Single */
.codehilite .ss { color: #19177C } /* Literal.String.Symbol */
.codehilite .bp { color: #008000 } /* Name.Builtin.Pseudo */
.codehilite .vc { color: #19177C } /* Name.Variable.Class */
.codehilite .vg { color: #19177C } /* Name.Variable.Global */
.codehilite .vi { color: #19177C } /* Name.Variable.Instance */
.codehilite .il { color: #666666 } /* Literal.Number.Integer.Long */

/* @end */

/* @group Image */

.image {
	display: block;
}

.image_B {
	margin-right: auto;
	margin-left: auto;
}

.image_L {
	float: left;
	margin-right: 18px;
	margin-bottom: 18px;
}

.image_R {
	float: right;
	margin-left: 18px;
	margin-bottom: 18px;
}

.image_list {
	display: block;
}

.image_list .image_L, .image_list .image_R {
	margin-bottom: 0px;
}

.image .image_background {
	display: block;
	-moz-border-radius-topleft: 4px;
	-webkit-border-top-left-radius: 4px;
	-moz-border-radius-topright: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-bottom-left-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	-webkit-border-bottom-right-radius: 4px;
}

.image_has_caption .image_background {
	-moz-border-radius-bottomleft: 0px;
	-webkit-border-bottom-left-radius: 0px;
	-moz-border-radius-bottomright: 0px;
	-webkit-border-bottom-right-radius: 0px;
}

.image a {
	overflow: hidden;
}

.image .image_caption {
	display: block;
	background-color: #181818;
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-bottom-left-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	-webkit-border-bottom-right-radius: 4px;
	margin-top: 1px;
	color: #999;
	font: 10px/10px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	padding: 1px 4px 2px;
}

.image a .icon {
	background: url(../images/image_expand_icon.gif) no-repeat right bottom;
	display: block;
	opacity: 0.70;
}

.image a:hover .icon {
	opacity: 1;
}

/* @end */



