/* @group Base Stuff */

* {
	margin: 0;
	padding: 0;
	border-style: none;
}

body {
	background: #000 url(/media/images/background.png) no-repeat left top;
	color: #ccc;
}



/* @end */

/* @group Global */

a.img, div.img {
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	display: block;
}

/* @end */

/* @group Header */

#header {
	padding-left: 120px;
	height: 160px;
	width: 560px;
	margin-bottom: 30px;
}

#header #logo {
	float: left;
	margin-right: 35px;
	position: relative;
	right: 3px;
}

#header .links {
	float: left;
	margin-left: 40px;
	font: 11px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	width: 130px;
	list-style-type: none;
	padding-top: 57px;
	text-shadow: #000 0 0 0;
}

#header .links li {
	list-style-type: none;
	margin-top: 11px;
}

#header .links a {
	color: #999;
	font-weight: normal;
	font-style: normal;
	text-decoration: none;
	width: 130px;
	display: block;
}

#header .links a:hover {
	color: #fff;
	font-weight: normal;
	font-style: normal;
	text-decoration: none;
	text-shadow: none;
}

#header .links a.active {
	color: #fff;
	font-weight: normal;
	font-style: normal;
	text-decoration: none;
}



/* @end */

/* @group Journal Entry */


#jornal_entry {
	padding-left: 120px;
	width: 770px;
}

#jornal_entry .main_block {
	width: 560px;
	float: left;
}

#jornal_entry h1 {
	width: 560px;
	font: 32px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #fff;
	text-shadow: #000 0 0 0;
}

#jornal_entry .info {
	width: 560px;
	font: 11px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #333;
	margin-top: 7px;
	margin-bottom: 19px;
	text-shadow: #000 0 0 0;
}

#jornal_entry .info a {
	color: #4b4b4b;
	text-decoration: none;
}

#jornal_entry .info a:hover {
	color: #656565;
}

#jornal_entry .extra_block {
	width: 180px;
	float: right;
}

#jornal_entry .extra_block p {
	color: #1e1e1e;
	font: 10px/12px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	padding-bottom: 12px;
}

#jornal_entry .extra_block ul {
	padding-bottom: 12px;
}

#jornal_entry .extra_block li {
	color: #1e1e1e;
	font: 10px/12px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	margin-left: 18px;
}

#jornal_entry .extra_block .images {
	padding-bottom: 12px;
}

#jornal_entry .extra_block .images a {
	opacity: 0.15;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	display: block;
	width: 36px;
	height: 36px;
	background-color: #989898;
	float: left;
	margin-right: 3px;
	margin-bottom: 3px;
}

#jornal_entry .extra_block .images a:hover {
	opacity: 0.50;
}

#jornal_entry .extra_block a {
	color: #2e2e2e;
	text-decoration: none;
}

#jornal_entry .extra_block a:hover {
	color: #666;
}

#jornal_entry .extra_block h5 {
	color: #333;
	font: bold 11px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: #000 0 0 0;
}

/* @end */

/* @group Footer */

#footer {
	padding-left: 120px;
	padding-top: 40px;
	padding-bottom: 30px;
	color: #333;
	font: 10px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	clear: both;
}



/* @end */

/* @group Comments */

#comments {
	margin-left: 120px;
	width: 560px;
	border-top: 1px solid #191919;
	clear: both;
	padding-top: 18px;
}

#comments .comment {
	width: 560px;
	clear: both;
	margin-bottom: 18px;
}

#comments .procomment {
	background-color: #111;
	padding-top: 18px;
	margin-bottom: 32px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

#comments .comment .info {
	width: 160px;
	float: left;
}

#comments .comment .content {
	width: 382px;
	float: right;
	padding-right: 18px;
}

#comments .comment .info h5.name {
	text-align: center;
	color: #fff;
	font: bold 16px/36px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: #000 0 0 0;
}

#comments .comment .info h6.date {
	text-align: center;
	color: #333;
	font: 10px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: #000 0 0 0;
}

#comments .comment .info h6.date a {
	color: #333;
	text-decoration: none;
}

#comments .comment .info h6.user_type {
	text-align: center;
	color: #333;
	font: 10px/18px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-shadow: #000 0 0 0;
}

#comments .procomment .info h6.user_type {
	margin-bottom: 18px;
}

#comments .comment .info h6.date a:hover {
	color: #666;
}

.clear{
	clear: both;
}

#commentform {
	margin-left: 120px;
	width: 560px;
	border-top: 1px solid #191919;
	clear: both;
	padding-top: 18px;
}

/* @end */

/* @group About Me */

#about_page {
	margin-left: 120px;
	width: 560px;
}



/* @end */

